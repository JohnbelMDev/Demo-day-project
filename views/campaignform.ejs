  <h1>Campaign Form</h1>
  <section class="container">
    <section class="childContainer">
      <p>Please feel out this form to process your information to our donors</p>
      <form action="/submission" enctype="multipart/form-data" method="post">
        <div class="form-group">
          <input type="text" name="fullName" class="form-control" id="formGroupExampleInput" placeholder="Full Name" required>
        </div>


        <div class="form-group">
          <input type="text" name="name" class="form-control" id="formGroupExampleInput" placeholder="Name" required>
        </div>


        <!-- <div class="form-group">
          <input type="text" name="fullName" class="form-control" id="formGroupExampleInput" placeholder="Fullname" required>
        </div> -->
        <!-- <div class="form-group">
          <input type="file" name="file-to-upload">
        </div> -->
        <div class="form-group">
          <input type="text" name="description" class="form-control" id="formGroupExampleInput2" placeholder="Description" required>
        </div>
        <div class="form-group">
          <input class="color" type="file" name="file-to-upload">
        </div>

        <div class="form-group">
          <input type="number"  name="amount" class="form-control" id="formGroupExampleInput2" placeholder="Amount to raise" required>
        </div>
        <div class="form-group">
          <input type="date" name="date" class="form-control" id="formGroupExampleInput2" placeholder="Date" required>
        </div>

        <div class="form-group">
          <input type="text" name="homeAddress" class="form-control" id="formGroupExampleInput2" placeholder="Home Address" required>
        </div>
        <div class="form-group">

          <input type="text" class="form-control" name="itemsTodonate" placeholder="Items to Donate, Use Commas">
          <br>
          <select class="" name="category">
                <option value="Education">Education</option>
                <option value="Tech">Tech</option>
                <option value="Food">Food</option>
                <option value="Environment">Environments</option>
                <option value="Clothes">Clothes</option>

          </select>
        </div>
        <button class="left01"type="submit" value="Upload">Submit</button>

      </form>
      <button> <a class="logout" href="/logout">Logout</a></button>

    </section>
  </section>


